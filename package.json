{
    "name": "@judgeq/simple-sandbox",
    "version": "0.0.1",
    "packageManager": "pnpm@7.0.0",
    "description": "A simple sandbox for Node.js using Linux namespaces and cgroup.",
    "author": "JudgeQ Develop Team",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/JudgeQ-Dev/simple-sandbox"
    },
    "keywords": [
        "sandbox",
        "chroot",
        "linux"
    ],
    "main": "./dist/index.mjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "require": "./dist/index.cjs",
            "import": "./dist/index.mjs",
            "types": "./dist/index.d.ts"
        }
    },
    "typesVersions": {
        "*": {
            "*": [
                "./dist/*",
                "./dist/index.d.ts"
            ]
        }
    },
    "files": [
        "src",
        "dist",
        "native",
        "CMakeLists.txt"
    ],
    "sideEffects": false,
    "scripts": {
        "install": "cmake-js compile --CDCMAKE_EXPORT_COMPILE_COMMANDS --cc=clang --cxx=clang++ --CDCMAKE_CXX_FLAGS=-stdlib=libc++ --CDCMAKE_EXE_LINKER_FLAGS=\"-stdlib=libc++ -lc++abi -fuse-ld=lld\"",
        "clean": "rm -rf ./build",
        "build": "unbuild",
        "dev": "unbuild --stub",
        "start": "esmo src/index.ts",
        "test": "vitest",
        "lint": "run-p lint:*",
        "lint:build": "tsc --noEmit ./src/*.ts",
        "lint:eslint": "eslint ./src/*.ts ./test/*.ts",
        "lint:style": "prettier --check ./src/*.ts ./test/*.ts",
        "format": "run-p format:*",
        "format:eslint": "eslint --fix ./src/*.ts ./test/*.ts",
        "format:style": "prettier --write ./src/*.ts ./test/*.ts",
        "prepublishOnly": "npm run build",
        "release": "bumpp --commit --push --tag && npm publish"
    },
    "dependencies": {
        "bindings": "^1.5.0",
        "cmake-js": "^6.3.0",
        "node-addon-api": "^3.2.1",
        "randomstring": "^1.2.2"
    },
    "devDependencies": {
        "@types/bindings": "^1.5.1",
        "@types/node": "^14.18.16",
        "@types/randomstring": "^1.1.8",
        "@typescript-eslint/eslint-plugin": "^5.22.0",
        "@typescript-eslint/parser": "^5.22.0",
        "bumpp": "^7.1.1",
        "eslint": "^8.15.0",
        "esmo": "^0.14.1",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.6.2",
        "rimraf": "^3.0.2",
        "typescript": "^4.6.4",
        "unbuild": "^0.7.4",
        "vite": "^2.9.8",
        "vitest": "^0.10.5"
    }
}
